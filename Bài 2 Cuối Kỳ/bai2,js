

    function merge2String(s1,s2){
        let String = 0;
        for(let i=0; i<s1.length; i++){
            for(let j=0; j<s2.length; i++){
                if(s1[i] === s1[i+1]){
                    String++;
                }
            }
        }
        return String;
        }
    merge2String("abc", "123");

    
  
